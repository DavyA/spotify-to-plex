{
  "name": "@spotify-to-plex/http-client",
  "version": "1.0.0",
  "description": "Shared HTTP client utilities for Spotify-to-Plex applications",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": "./dist/*/*.d.ts",
      "default": "./dist/*/*.js"
    },
    "./*/*/*": {
      "types": "./dist/*/*/*.d.ts",
      "default": "./dist/*/*/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}