{
    "name": "@jjdenhertog/plex-music-search",
    "version": "1.0.23",
    "description": "Library to find tracks in Plex",
    "keywords": [
        "plex",
        "music",
        "search"
    ],
    "homepage": "https://github.com/jjdenhertog/plex-music-search",
    "license": "MIT",
    "main": "./dist/index.js",
