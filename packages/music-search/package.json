{
    "name": "@spotify-to-plex/music-search",
    "version": "1.0.0",
    "description": "Library to find tracks in a music library.",
    "keywords": [
        "search",
        "music"
    ],
    "license": "MIT",
    "exports": {
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./dist/*.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@spotify-to-plex/shared-types": "workspace:*",
        "fs-extra": "^11.3.1",
        "string-similarity-js": "^2.1.4"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.3.0",
        "typescript": "^5.0.0"
    }
}