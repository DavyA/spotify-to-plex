{
  "name": "@spotify-to-plex/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for Spotify-to-Plex applications",
  "exports": {
    "./common/MatchFilterConfig": {
      "types": "./dist/common/MatchFilterConfig.d.ts",
      "default": "./dist/common/MatchFilterConfig.js"
    },
    "./common/*": {
      "types": "./dist/common/*.d.ts",
      "default": "./dist/common/*.js"
    },
    "./dashboard/*": {
      "types": "./dist/dashboard/*.d.ts",
      "default": "./dist/dashboard/*.js"
    },
    "./plex/*": {
      "types": "./dist/plex/*.d.ts",
      "default": "./dist/plex/*.js"
    },
    "./spotify/*": {
      "types": "./dist/spotify/*.d.ts",
      "default": "./dist/spotify/*.js"
    },
    "./tidal/*": {
      "types": "./dist/tidal/*.d.ts",
      "default": "./dist/tidal/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}