        "@spotify-to-plex/shared-types": "workspace:*",
        "axios": "^1.0.0"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    },
    "exports": {
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./dist/*.js"
        }
    }
