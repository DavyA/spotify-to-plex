{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "jsx": "preserve",
    
    "types": ["react", "react-dom", "node"],
    
    "allowJs": true,
    "noEmit": false
  }
}